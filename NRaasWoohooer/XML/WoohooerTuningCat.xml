<?xml version="1.0" encoding="utf-8"?>
<base>
  <Current_Tuning>
    <kLikingGatingForAutonomousWoohoo value="40">
      <!--Range: 0-100. Minimum liking gate for performing autonomous actions-->
    </kLikingGatingForAutonomousWoohoo>
    <kRiskyBabyMadeChance value="10">
      <!--Range: 0-100. Chance of Sim getting Pregnant on RiskyWoohoo.-->
    </kRiskyBabyMadeChance>
    <kWoohooAutonomous value="False">
      <!--Whether regular woohoo is autonomous-->
    </kWoohooAutonomous>
    <kRiskyAutonomous value="False">
      <!--Whether risky woohoo is autonomous-->
    </kRiskyAutonomous>
    <kTryForBabyAutonomous value="False">
      <!--Whether Try For Baby is autonomous-->
    </kTryForBabyAutonomous>
    <kMustBeRomanticForAutonomous value="True">
      <!--Whether a couple must be flagged as romantic before considering woohoo-->
    </kMustBeRomanticForAutonomous>
    <kAllowNearRelationWoohoo value="False">
      <!--Whether to allow near relations to woohoo-->
    </kAllowNearRelationWoohoo>
    <kAllowNearRelationWoohooAutonomous value="False">
      <!--Whether to allow near relations to autonomously woohoo-->
    </kAllowNearRelationWoohooAutonomous>
    <kAutonomousMaleMaleTryForBaby value="False">
      <!--Whether to allow try for baby between male couples-->
    </kAutonomousMaleMaleTryForBaby>
    <kAutonomousFemaleFemaleTryForBaby value="True">
      <!--Whether to allow try for baby between female couples-->
    </kAutonomousFemaleFemaleTryForBaby>
    <kAllowSameSexTryForBaby value="True">
      <!--Whether to display try for baby for same-sex couples-->
    </kAllowSameSexTryForBaby>
    <kMaximumHouseholdSizeForAutonomous value="6">
      <!--Maximum number of sims per household for autonomous pregnancy-->
    </kMaximumHouseholdSizeForAutonomous>
    <kWoohooBaseChanceScoring value="25">
      <!--Range: -200 to 200. The base chance of a sim considering a Woohoo interaction-->
    </kWoohooBaseChanceScoring>
