    </kAllowOffLotRiskyAutonomous>
    <kAllowOffLotTryForBabyAutonomous value="True">
      <!--Whether to allow sims to engage in autonomous Try For Baby while not on the active lot-->
    </kAllowOffLotTryForBabyAutonomous>

