      <!--Range: -200 to 200. The base chance of a sim being interested in a romantic interaction-->
    </kRomanceBaseChanceScoring>
    <kAttractionBaseChanceScoring value="0">
      <!--Range: -200 to 200. The base chance of a sim being attracted to another-->
    </kAttractionBaseChanceScoring>
